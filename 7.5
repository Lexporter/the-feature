public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		
		int a[] = new int[101];
		int b[] = new int[101];
		int sum = 0;
		
		System.out.print("Enter the integers between 1 and 100:");
		
		int cnt = input.nextInt();
		
		while(cnt != 0) {
			a[sum++] = cnt;
			b[cnt]++;
			cnt = input.nextInt();
		}
		
		for(int i = 0; i<sum;i++) {
			System.out.println(a[i] + " occurs "+ b[a[i]] +" times");
		}
	}
